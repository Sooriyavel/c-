#include <iostream>
using namespace std;
int main() {
    int num1, num2;

    cout << "Enter the first integer: ";
    cin >> num1;
    cout << "Enter the second integer: ";
    cin >> num2;

    num1 = num1 ^ num2;  
    num2 = num1 ^ num2;
    num1 = num1 ^ num2;

    cout << "After swapping, the first integer is: " << num1 << endl;
    cout << "After swapping, the second integer is: " << num2 << endl;

    return 0;
}
