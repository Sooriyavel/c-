#include<iostream>
using namespace std;
int main(){
  
  char a;
  
  cout<<"enter the charcter :\n ";
  cin>>a;
  
  if(a=='a'||a=='e'||a=='i'||a=='o'||a=='u'||a=='A'||a=='E'||a=='I'||a=='O'||a=='U')
    cout<<"the given char in vowel";
  else 
    cout<< "the given char is consonant";
  
  return 0;
  
}
