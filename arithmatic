#include <iostream>
using namespace std;
main() {
   int a = 21;
   int b = 10;
   int c ;
   c = a + b;
   cout << "Line 1 - Value of add is :" << c << endl ;
   
   c = a - b;
   cout << "Line 2 - Value of sub is  :" << c << endl ;
   
   c = a * b;
   cout << "Line 3 - Value of multiple is :" << c << endl ;
   
   c = a / b;
   cout << "Line 4 - Value of divide is  :" << c << endl ;
   
   c = a % b;
   cout << "Line 5 - Value of modulo is  :" << c << endl ;
   return 0;
}
